#!/bin/bash -e

UDMI_ROOT=$(dirname $0)/..
cd $UDMI_ROOT

source etc/shell_common.sh

if [[ $# != 1 ]]; then
   usage PROJECT_SPEC
fi

project_spec=$1
shift

site_path=sites/udmi_site_model

bin/registrar $site_path $project_spec -d

bin/registrar $site_path $project_spec GAT-123

more $site_path/out/registration_summary.csv

pubber_bg GAT-123

bin/mapper GAT-123 discover
