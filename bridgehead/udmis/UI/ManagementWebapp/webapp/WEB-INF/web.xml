<web-app>
    <servlet>
        <servlet-name>ManagerServlet</servlet-name>
        <servlet-class>webapp.ManagerServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ManagerServlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>/css/*</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>/js/*</url-pattern>
    </servlet-mapping>
</web-app>