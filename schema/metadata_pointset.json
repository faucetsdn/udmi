{
  "title": "Pointset Metadata",
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "additionalProperties": false,
  "properties": {
    "points": {
      "additionalProperties": false,
      "maxProperties": 150,
      "existingJavaType": "java.util.HashMap<String, PointPointsetMetadata>",
      "patternProperties": {
        "^[a-z][a-z0-9]*(_[a-z0-9]+)*$": {
          "$ref": "file:metadata_pointset_point.json#"
        }
      }
    }
  },
  "required": [
    "points"
  ]
}
