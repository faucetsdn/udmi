{
  "title": "Pointset State",
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "additionalProperties": false,
  "properties": {
    "state_etag": {
      "type": "string",
      "maxLength": 32
    },
    "points": {
      "additionalProperties": false,
      "existingJavaType": "java.util.HashMap<String, PointPointsetState>",
      "patternProperties": {
        "^[a-z][a-z0-9]*(_[a-z0-9]+)*$": {
          "$ref": "file:state_pointset_point.json#"
        }
      }
    }
  },
  "required": [
    "points"
  ]
}
