{
  "title": "pointset state snippet",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "etag": {
      "type": "string"
    },
    "points": {
      "additionalProperties": false,
      "patternProperties": {
        "^[a-z][a-z0-9]*(_[a-z0-9]+)*$": {
          "additionalProperties": false,
          "properties": {
            "value_state": {
              "enum": [
                "applied",
                "updating",
                "overridden",
                "invalid",
                "failure"
              ]
            },
            "status": {
              "$ref": "file:common.json#/definitions/entry"
            }
          }
        }
      }
    }
  },
  "required": [
    "points"
  ]
}
