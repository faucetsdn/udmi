{
  "title": "Pointset Event",
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "additionalProperties": false,
  "properties": {
    "timestamp": {
      "description": " RFC 3339 timestamp the telemetry event was generated",
      "type": "string",
      "format": "date-time",
      "examples": ["2019-01-17T14:02:29.364Z"]
    },
    "version": {
      "description": "Major version of the UDMI schema",
      "type": "integer",
      "minimum": 1,
      "maximum": 1
    },
    "points": {
      "additionalProperties": false,
      "existingJavaType": "java.util.HashMap<String, PointPointsetEvent>",
      "patternProperties": {
        "^[a-z][a-z0-9]*(_[a-z0-9]+)*$": {
          "$ref": "file:event_pointset_point.json#"
        }
      }
    }
  },
  "required": [
    "timestamp",
    "version",
    "points"
  ]
}
