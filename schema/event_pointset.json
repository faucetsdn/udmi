{
  "title": "Pointset Event",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "version": {
      "type": "integer",
      "minimum": 1,
      "maximum": 1
    },
    "points": {
      "additionalProperties": false,
      "existingJavaType": "java.util.HashMap<String, PointPointsetEvent>",
      "patternProperties": {
        "^[a-z][a-z0-9]*(_[a-z0-9]+)*$": {
          "$ref": "file:event_pointset_point.json#"
        }
      }
    }
  },
  "required": [
    "timestamp",
    "version",
    "points"
  ]
}
