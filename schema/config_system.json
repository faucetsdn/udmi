{
  "title": "System Config",
  "description": "[Read more](../../docs/messages/system.md)",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "min_loglevel": {
      "description": "The minimum loglevel for reporting log messages below which log entries should not be sent. Default to 300.",
      "type": "integer",
      "minimum": 100,
      "maximum": 800
    },
    "auth_key": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "private": {
          "type": "string"
        }
      },
      "required": [
        "private"
      ]
    }
  }
}
