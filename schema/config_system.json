{
  "title": "System Config",
  "description": "[System Config Documentation](../docs/messages/system.md#config)",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "min_loglevel": {
      "description": "The minimum loglevel for reporting log messages below which log entries should not be sent. Default to INFO.",
      "type": "string",
      "enum": [ "ERROR", "WARN", "INFO", "DEBUG" ],
      "examples": ["INFO"]
    }
  }
}
