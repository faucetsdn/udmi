{
  "title": "Cloud Metadata",
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "additionalProperties": false,
  "properties": {
    "auth_type": {
      "enum": [
        "ES256",
        "ES256_X509",
        "RS256",
        "RS256_X509"
      ]
    },
    "device_key": {
      "type": "boolean"
    },
    "is_gateway": {
      "type": "boolean"
    }
  },
  "required": [
    "auth_type"
  ]
}
