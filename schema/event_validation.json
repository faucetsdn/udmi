{
  "title": "Validation Event",
  "description": "Validation result",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "timestamp": {
      "description": "RFC 3339 timestamp the discover telemetry event was generated",
      "type": "string",
      "format": "date-time",
      "examples": ["2019-01-17T14:02:29.364Z"]
    },
    "version": {
      "description": "Version of the UDMI schema",
      "type": "string"
    },
    "last_updated": {
      "description": "Last time this validation report was updated",
      "type": "string"
    },
    "site_id": {
      "description": "Site id being validated",
      "type": "string"
    },
    "device_id": {
      "description": "Device id that has been validated",
      "type": "string"
    },
    "subfolder": {
      "description": "Subfolder of the validated message",
      "type": "string"
    },
    "subtype": {
      "description": "Subtype of the validated message",
      "type": "string"
    },
    "extra_devices": {
    },
    "devices": {
    }
  },
  "required": [
    "timestamp",
    "version",
    "site_id"
  ]
}
