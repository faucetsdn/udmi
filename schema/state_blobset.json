{
  "title": "Blobset State",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "state_etag": {
      "type": "string",
      "maxLength": 32
    },
    "blobs": {
      "additionalProperties": false,
      "existingJavaType": "java.util.HashMap<String, BlobBlobsetState>",
      "patternProperties": {
        "^[a-z][a-z0-9]*(_[a-z0-9]+)*$": {
          "$ref": "file:state_blobset_blob.json#"
        }
      }
    }
  },
  "required": [
    "blobs"
  ]
}
