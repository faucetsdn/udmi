{
  "$id": "https://raw.githubusercontent.com/faucetsdn/udmi/1.4.1/schema/state_blobset.json",
  "title": "Blobset State",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "blobs": {
      "additionalProperties": false,
      "existingJavaType": "java.util.HashMap<String, BlobBlobsetState>",
      "patternProperties": {
        "^_?[a-z][a-z0-9]*(_[a-z0-9]+)*$": {
          "$ref": "file:state_blobset_blob.json#"
        }
      }
    }
  },
  "required": [
    "blobs"
  ]
}
