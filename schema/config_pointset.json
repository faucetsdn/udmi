{
  "title": "Pointset Config",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "state_etag": {
      "type": "string",
      "maxLength": 32
    },
    "set_value_expiry": {
      "type": "string",
      "format": "date-time"
    },
    "sample_limit_sec": {
      "type": "integer",
      "minimum": 1,
      "maximum": 86400
    },
    "sample_rate_sec": {
      "type": "integer",
      "minimum": 1,
      "maximum": 86400
    },
    "points": {
      "additionalProperties": false,
      "existingJavaType": "java.util.HashMap<String, PointPointsetConfig>",
      "patternProperties": {
        "^[a-z][a-z0-9]*(_[a-z0-9]+)*$": {
          "$ref": "file:config_pointset_point.json#"
        }
      }
    }
  }
}
