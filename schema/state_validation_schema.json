{
  "$udmi_version": "1.4.1",
  "title": "Schema Validation State",
  "description": "Schema validation state",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "stages": {
      "existingJavaType": "java.util.Map<udmi.schema.FeatureEnumeration.FeatureStage, Entry>",
      "additionalProperties": false,
      "patternProperties": {
        "^[_a-z]+$": {
          "status": {
            "$ref": "file:entry.json"
          }
        }
      }
    },
    "sequences": {
      "existingJavaType": "java.util.Map<String, SequenceValidationState>",
      "additionalProperties": false,
      "patternProperties": {
        "^[_a-z]+$": {
          "$ref": "file:state_validation_sequence.json"
        }
      }
    }
  }
}
