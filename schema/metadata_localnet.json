{
  "title": "Localnet Metadata",
  "description": "Used to describe device local network parameters",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "subsystem": {
      "additionalProperties": false,
      "existingJavaType": "java.util.HashMap<String, SubsystemLocalnetMetadata>",
      "patternProperties": {
        "^[a-z0-9-]+$": {
          "$ref": "file:metadata_localnet_subsystem.json"
        }
      }
    }
  },
  "required": [
    "subsystem"
  ]
}
