{
  "title": "Blobset Config",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "blobs": {
      "additionalProperties": false,
      "existingJavaType": "java.util.HashMap<String, BlobBlobsetConfig>",
      "patternProperties": {
        "^[a-z][a-z0-9]*(_[a-z0-9]+)*$": {
          "$ref": "file:config_blobset_blob.json#"
        }
      }
    }
  }
}
