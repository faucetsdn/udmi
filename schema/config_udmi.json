{
  "$udmi_version": "1.4.1",
  "title": "Udmi Config",
  "description": "Config for a UDMI reflector client",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "last_state": {
      "type": "string",
      "format": "date-time"
    },
    "setup": {
      "title": "Setup Udmi Config",
      "type": "object",
      "properties": {
        "functions_min": {
          "type": "integer"
        },
        "functions_max": {
          "type": "integer"
        },
        "udmi_version": {
          "type": "string"
        },
        "udmi_functions": {
          "type": "string"
        },
        "deployed_at": {
          "type": "string",
          "format": "date-time"
        },
        "deployed_by": {
          "type": "string"
        }
      }
    }
  }
}
