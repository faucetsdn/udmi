{
  "flow_defaults": {
    "protocol": "pubsub",
    "hostname": "${GCP_PROJECT}",
    "distributor": "stately"
  },
  "flows": {
    "target": {
      "recv_id": "udmi_target-udmis"
    },
    "state": {
      "recv_id": "udmi_state-udmis",
      "send_id": "udmi_target"
    },
    "reflect": {
      "recv_id": "udmi_reflect-udmis",
      "send_id": "udmi_target"
    }
  },
  "iot_access": {
    "iot-access": {
      "provider": "dynamic",
      "project_id": "gcp-iot-core,clearblade-iot-core"
    },
    "clearblade-iot-core": {
      "provider": "clearblade",
      "project_id": "${CLEARBLADE_PROJECT}"
    },
    "gcp-iot-core": {
      "provider": "gcp",
      "project_id": "${GCP_PROJECT}",
      "options": "${GCP_IOT_OPTIONS}"
    }
  },
  "distributors": {
    "stately": {
      "protocol": "mqtt",
      "transport": "tcp",
      "hostname": "udmis-broker",
      "port": "1883"
    }
  }
}
