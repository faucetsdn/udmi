{
  "flow_defaults": {
    "protocol": "pubsub",
    "hostname": "${GCP_PROJECT}"
  },
  "flows": {
    "target": {
      "recv_id": "udmi_target-udmis",
      "distributor": "stately"
    },
    "state": {
      "recv_id": "udmi_state-udmis",
      "send_id": "udmi_target",
      "distributor": "stately"
    },
    "reflect": {
      "recv_id": "udmi_reflect-udmis",
      "send_id": "udmi_target",
      "distributor": "stately"
    }
  },
  "iot_access": {
    "iot-access": {
      "provider": "dynamic",
      "project_id": "gcp-iot-core,clearblade-iot-core"
    },
    "clearblade-iot-core": {
      "provider": "clearblade",
      "project_id": "${CLEARBLADE_PROJECT}"
    },
    "gcp-iot-core": {
      "provider": "gcp",
      "project_id": "${GCP_PROJECT}"
    }
  },
  "distributors": {
    "stately": {
      "protocol": "iot_access"
    }
  }
}
