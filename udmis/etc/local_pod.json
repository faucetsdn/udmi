{
  "include": "prod_pod.json",
  "flow_defaults": {
    "protocol": "mqtt",
    "transport": "ssl",
    "hostname": "localhost",
    "msg_prefix": "/r/{registryId}/d/{deviceId}/",
    "auth_provider": {
      "basic": {
        "username": "rocket",
        "password": "monkey"
      }
    }
  },
  "flows": {
    "state": {
      "recv_id": "state",
      "send_id": "state/{subFolder}"
    },
    "distributor": {
      "enabled": "false"
    }
  },
  "iot_access": {
    "implicit": {
      "provider": "implicit",
      "options": "enabled=${ETCD_CLUSTER}"
    }
  }
}
