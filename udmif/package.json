{
  "name": "udmif",
  "version": "1.0.0",
  "description": "simple package allowing us to run both sub projects with a single command",
  "scripts": {
    "web-build": "npm --prefix web/ install",
    "web": "npm --prefix web/ run start",
    "api-build": "npm --prefix api/ install",
    "api": "npm --prefix api/ run dev",
    "start-all": "npm-run-all --parallel api web",
    "build-all": "npm-run-all --parallel api-build web-build"
  },
  "author": "Buildings IOT",
  "license": "ISC"
}