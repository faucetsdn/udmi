#!/bin/bash -e

UDMI_ROOT=$(realpath $(dirname $(readlink -f $0))/../..)

source $UDMI_ROOT/etc/shell_common.sh

source $UDMI_ROOT/etc/find_site_model_profile.sh

if [[ -z ${profile} ]]; then
    false
fi

$UDMI_ROOT/validator/bin/build
jarfile=${UDMI_ROOT}/validator/build/libs/validator-1.0-SNAPSHOT-all.jar
java -cp $jarfile com.google.daq.mqtt.validator.Validator ${profile} "$@"
